<local:MovableElement x:Class="PanoramicData.view.filter.CombinedFilterHolder"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PanoramicData.view.filter"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>
        <Grid Name="left" Column="0" VerticalAlignment="Top" Margin="0, 0, -3, 0"/>
        <Grid Name="center" Column="1"/>
        <Grid Name="right" Column="2" VerticalAlignment="Top" Margin="-3, 0, 0, 0"/>
    </Grid>
</local:MovableElement>
