<Window x:Class="NFLScraper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button Content="Parse" Click="Parse_Click" Grid.Row="0"/>
        <Button Content="Save" Click="Save_Click" Grid.Row="1"/>

    </Grid>
</Window>
