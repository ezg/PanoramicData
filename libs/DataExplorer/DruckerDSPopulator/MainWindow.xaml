<Window x:Class="DruckerDSPopulator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button Content="Drucker" Click="Drucker_Click" Grid.Row="0"/>
        <Button Content="Adult" Click="Adult_Click" Grid.Row="1"/>
        <Button Content="CS" Click="CS_Click" Grid.Row="2"/>
    </Grid>
</Window>
