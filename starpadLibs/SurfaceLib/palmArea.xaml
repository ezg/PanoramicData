<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="SurfaceLib.palmArea"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="480">

	<Grid x:Name="LayoutRoot">
		<Ellipse Opacity="0.2" Margin="-99,-92,0,0" Stroke="{x:Null}" HorizontalAlignment="Left" VerticalAlignment="Top" Width="187" Height="163">
			<Ellipse.Fill>
				<RadialGradientBrush SpreadMethod="Pad">
					<GradientStop Color="#FF00FF58" Offset="0"/>
					<GradientStop Color="#FF114C60" Offset="1"/>
					<GradientStop Color="#FF00FFC2" Offset="0"/>
				</RadialGradientBrush>
			</Ellipse.Fill>
		</Ellipse>
	</Grid>
</UserControl>