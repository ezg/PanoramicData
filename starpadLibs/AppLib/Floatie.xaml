<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="starPadSDK.AppLib.Floatie"
	x:Name="UserControl" Height="64" Width="Auto" Background="{x:Null}" Opacity="1" HorizontalAlignment="Left" VerticalAlignment="Top">
	<UserControl.Resources>
		
	</UserControl.Resources>
	<UserControl.Triggers>
	</UserControl.Triggers>

	<Grid x:Name="LayoutRoot" HorizontalAlignment="Left">
		<Rectangle HorizontalAlignment="Left" Margin="0,39,0,0" VerticalAlignment="Top" Width="25" Height="25" Stroke="{x:Null}" d:LayoutOverrides="VerticalAlignment" x:Name="Shadow">
			<Rectangle.Fill>
				<RadialGradientBrush MappingMode="Absolute" SpreadMethod="Pad" Center="12.5,12.5" GradientOrigin="12.5,12.5" RadiusX="12.5" RadiusY="12.5">
					<GradientStop Color="#00000000" Offset="0.987"/>
					<GradientStop Color="#FF00FF00" Offset="0"/>
				</RadialGradientBrush>
			</Rectangle.Fill>
		</Rectangle>
		<DockPanel VerticalAlignment="Top" Height="33" LastChildFill="True" Margin="0,2,0,0" Width="Auto" HorizontalAlignment="Left" x:Name="dockPanel"/>
    </Grid>
</UserControl>