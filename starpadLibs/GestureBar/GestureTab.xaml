			<Rectangle Stroke="{x:Null}" RadiusX="5.72" RadiusY="5.72" x:Name="ButtonBase2" Width="{Binding Path=ActualWidth, ElementName=UserControl, Mode=Default}" Height="29.25" Canvas.Top="32.75" RenderTransformOrigin="0.5,0.5">
