	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="starPadSDK.GestureBarLib.Icon_SelectContext"
	x:Name="UserControl"
	Width="28.333" Height="28.5">

	<Grid x:Name="LayoutRoot">
		<Canvas x:Name="SelectContextIcon" Margin="0,0,-10.917,-2">
			<Rectangle Stroke="#FF000000" Width="14.75" Height="7.5" Canvas.Left="8" Canvas.Top="12.5">
				<Rectangle.Fill>
					<LinearGradientBrush EndPoint="0.75,1.213" StartPoint="0.25,-0.213">
						<GradientStop Color="#FFF42424" Offset="0"/>
						<GradientStop Color="#FF6F0000" Offset="1"/>
					</LinearGradientBrush>
				</Rectangle.Fill>
			</Rectangle>
		</Canvas>
	</Grid>
</UserControl>