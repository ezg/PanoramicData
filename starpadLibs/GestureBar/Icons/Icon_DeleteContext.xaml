<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="starPadSDK.GestureBarLib.Icon_DeleteContext"
	x:Name="UserControl"
	Width="20" Height="22">

	<Grid x:Name="LayoutRoot">
		<Canvas x:Name="DeleteContext" Margin="0,0,1,-0.5">
			<Rectangle Stroke="#FF000000" Width="10.5" Height="17.75" RenderTransformOrigin="0.286,0.451" Canvas.Left="9" Canvas.Top="0.125">
				<Rectangle.Fill>
					<LinearGradientBrush EndPoint="0.75,1.213" StartPoint="0.25,-0.213">
						<GradientStop Color="#FF36F424" Offset="0"/>
						<GradientStop Color="#FF074E00" Offset="1"/>
					</LinearGradientBrush>
				</Rectangle.Fill>
			</Rectangle>
			<Ellipse Stroke="#FF000000" Width="12.75" Height="12.75" Canvas.Top="9.125">
				<Ellipse.Fill>
					<LinearGradientBrush EndPoint="0.75,1.213" StartPoint="0.25,-0.213">
						<GradientStop Color="#FF00B3FF" Offset="0"/>
						<GradientStop Color="#FF00066F" Offset="1"/>
					</LinearGradientBrush>
				</Ellipse.Fill>
			</Ellipse>
		</Canvas>
	</Grid>
</UserControl>