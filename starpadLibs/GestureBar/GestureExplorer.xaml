			</Canvas>
		</Canvas>
		<GestureBarLib:GestureExplorerTab HorizontalAlignment="Left" Margin="8,39,0,0" VerticalAlignment="Top" x:Name="Tab1" OnSelect="Tab1_OnSelect"/>
