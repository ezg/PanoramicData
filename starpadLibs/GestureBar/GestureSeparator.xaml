<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="starPadSDK.GestureBarLib.GestureSeparator"
	x:Name="UserControl"
	Width="1.209" Height="64">

	<Grid x:Name="LayoutRoot">
		<Rectangle Opacity="1" Stroke="{x:Null}" Margin="0,1.375,0.11,1.75">
			<Rectangle.Fill>
				<LinearGradientBrush EndPoint="0.5,0.016" StartPoint="0.5,0.975">
					<GradientStop Color="#00FFFFFF" Offset="0"/>
					<GradientStop Color="#00FFFFFF" Offset="1"/>
					<GradientStop Color="#FFFFFFFF" Offset="0.587"/>
				</LinearGradientBrush>
			</Rectangle.Fill>
		</Rectangle>
	</Grid>
</UserControl>